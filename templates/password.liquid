    @font-face {
      font-family: 'BodoniEgyptianPro-Light';
      src: url({{'33AE99_0_0.eot' | asset_url }});
      src: url({{'33AE99_0_0.eot?#iefix' | asset_url }}) format('embedded-opentype'),
