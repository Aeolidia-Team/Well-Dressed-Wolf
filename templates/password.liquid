      }

    * {
      margin: 0;
      padding: 0;
    }

    img {
      border: 0;
    }

    body,
    html {
      height: 100%;
    }
    
    body {
      margin: 0;
      /*border: 1px solid #f1f1f1;*/
      background-color: #fff;
    
      font: 1em/1.4em "Lucida Grande", "Trebuchet MS", Helvetica, sans-serif;
      color: #969da8;
    }

    h1,h2,h3,h4,h5,h6 
      color: #969da8;
      margin: 5px 0 10px 0;
      font-weight: 400;
    }

    h3,h4,h5,h6 {
      font-weight: normal;
    }

    a {
      color: #969da8;
      text-decoration: underline;
    }

    a:hover {
      color: #000;
    }

    p {
      color: #969da8;
      margin-bottom: 13px;
      font-size: 14px;
    }

    ul, ol {
      margin-bottom: 13px;
      margin-left: 15px;
    }

    ol {
      margin-left: 25px;
    }

    input.btn {
      font-weight: bold;
      cursor: pointer;
      border: 1px solid #bbb;
      border-top: 1px solid #ccc;
      border-bottom: 1px solid #aaa;
      background-color: #ddd;
      background-image: url('data:image/jpeg;base64,');
      background-position: top left;
      background-repeat: repeat-x;
      padding: 4px 6px;
      -moz-border-radius: 5px;
      -webkit-border-radius: 5px;
      border-radius: 5px;
      text-shadow: 0px 1px 1px #fff;
      color: #3a3a3a;
    }
    
    a,
    a:link,
    a:active,
    a:hover,
    a:visited {
     color: #969da8;
     text-decoration: underline;
    }

    .fa {
      font-size: 22px;
      margin: 0 3px;
    }
    
    input.btn:hover {
      -webkit-box-shadow: 0px 0px 3px #3084CA;
      -moz-box-shadow: 0px 0px 3px  #3084CA;
    }

    input.btn:active, input.btn:focus {
      outline: none;
      -moz-outline-style: none;
      background: #eee;
      border-top: 1px solid #aaa;
      border-bottom: 1px solid #ccc;
      color: #555;
    }

    #container {
      /*width: 500px;*/
     
      margin: 50px auto 0 auto;
    }

    
    #login {
      text-align: center;
     /* background: #fff;
      border: none;
      border-bottom: 1px solid #ccc;
      margin: 50px auto 0 auto;
      padding: 30px;
      width: 450px;
      -webkit-border-radius: 4px;
      -moz-border-radius: 4px;
      border-radius: 4px;
      box-shadow: 0 0 10px rgba(0,0,0,0.1);
      -moz-box-shadow: 0 0 10px rgba(0,0,0,0.1);
      -webkit-box-shadow: 0 0 10px rgba(0,0,0,0.1);
      box-shadow: 0 0 10px rgba(0,0,0,0.1);*/
    }
    
    #login p {
      margin: 0 0 1em 0;
      font-family: 'BodoniEgyptianPro-Light';
      font-size: 17px;
      color: #969da8;
      line-height: 2;
    }

    #login p em {
      font-size: 15px;
    }
    
    #login h1 {
      margin: 0 0 18px 0;
      text-align: center;
      line-height: 30px;
      font-family: 'BodoniEgyptianPro-Light';
      font-weight: normal;
    }
    
    #login img {
      margin: 0 auto;
      /*max-width: 300px;*/
    }

    #login .default-text {
      font-family: serif;
      font-style: italic;
      margin-bottom: 1.5em;
      font-size: 17px;
      display: block;
      text-align: center;
      margin-top: 33px;
    }

    form#login_form {
     /* border-top: 1px solid #e0e0e0; */
      padding-top: 25px;
      /* margin-top: 25px; */
      font-size: 80%;
      /*margin-bottom: 35px;*/
      width: 235px;
      margin: 0 auto;
    }

    form#login_form label {
      color: #777;
    }

    #login_form #password {
      font-size: 14px;
      padding: 4px 6px;
      border: 1px solid #ccc;
      outline: none;
      background: #e9e9e9;
      background: -moz-linear-gradient(top, #ececec 0%, #fff 100%);
      background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#ececec), color-stop(100%,#fff));
    }

    #login_form label, #login_form input {
      float: left;
      margin-right: 3px;
    }

    #footer {
      display: none;
      
      margin-top: 15px;
      margin-bottom: 40px;
      font-size: 13px;
      color: #969da8;
      text-shadow: 0 1px rgba(255,255,255,0.4);
      text-align: center;
    }

    #powered {
      color: #9c9c9c;
      display: block;
      margin-top: 15px;
    }

    #powered a {
      background-repeat: no-repeat;
      background-image: url(data:image/png;base64,);
      padding-left: 21px;
      color: #9c9c9c;
      text-decoration: none;
    }

    #powered a:hover {
      text-decoration: underline;
    }

    .errors {
      background: #feebeb;
      padding: 4px 5px;
      font-size: 15px;
      color: #7e1412;
      display: block;
      margin-bottom: 15px;
    }

    .errors ul {
      list-style: none;
      padding: 0;
      margin: 0;
    }

    .center {
      text-align: center;
      display: block;
      font-size: 17px;
    }
    
    .logo {
      margin-bottom: 30px;
    }

    .logo img {
      width: 300px;
    } 

    @media only screen and (min-width: 500px) {
      .logo img {
        width: 400px;
      } 
    }

    p.message {
    }  
  
